{
  "name": "pem-file",
  "version": "1.0.1",
  "description": "PEM file format encoder/decoder.",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "test": "xo && jest test.js",
    "test-ci": "xo && jest --coverage test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reklatsmasters/pem-file.git"
  },
  "keywords": [
    "pem",
    "rsa",
    "private",
    "public",
    "key"
  ],
  "author": "Dmitriy Tsvettsikh <me@reklatsmasters.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reklatsmasters/pem-file/issues"
  },
  "homepage": "https://github.com/reklatsmasters/pem-file#readme",
  "devDependencies": {
    "jest": "^23.6.0",
    "xo": "^0.20.3"
  },
  "xo": {
    "semicolon": false,
    "space": 2,
    "rules": {
      "object-curly-spacing": [
        "error",
        "always"
      ]
    }
  },
  "jest": {
    "modulePaths": [
      "<rootDir>"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "/test/",
      "/example/"
    ]
  }
}
